{
  "name": "node-read-zip",
  "version": "1.0.0",
  "description": "Simple project that read a .tar.gz file and execute js code inside",
  "main": "./dist/index.js",
  "repository": "https://github.com/jgfurlan2/node-read-zip",
  "license": "MIT",
  "scripts": {
    "once": "ts-node ./src/index.ts",
    "build": "yarn babel ./src --out-dir ./dist --extensions \".ts,.js\" --copy-files --no-copy-ignored",
    "lint": "eslint ./src --ext ts,tsx",
    "tgz:create": "cd ./example && tar -zcvf ../archive.tar.gz . && cd .."
  },
  "engines": {
    "node": "18.17.0",
    "yarn": "1.22.21"
  },
  "dependencies": {
    "tar-stream": "^3.1.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "18.17.0",
    "@types/tar-stream": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.3.3"
  }
}
